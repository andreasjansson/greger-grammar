    "license": "MIT",
    "description": "greger.el parser",
    "authors": [
      {
        "name": "Andreas Jansson",
        "email": "andreas.s.t.jansson@gmail.com",
        "url": "https://andreasjansson.se/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-greger"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
