        if (lexer->lookahead == '{') {
            // Found opening brace, increment counter
            brace_count++;
            if (!iswspace(lexer->lookahead)) {
                has_non_whitespace = true;
            }
            advance(lexer);
            has_content = true;
            lexer->mark_end(lexer);
        } else if (lexer->lookahead == '}') {
            if (brace_count > 0) {
                // This is a closing brace for a nested opening brace
                brace_count--;
                if (!iswspace(lexer->lookahead)) {
                    has_non_whitespace = true;
                }
                advance(lexer);
                has_content = true;
                lexer->mark_end(lexer);
