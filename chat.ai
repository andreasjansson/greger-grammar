                                            scanner->in_tool_content = false;
                                            scanner->tool_id[0] = '\0';
                                            lexer->result_symbol = TOOL_CONTENT;
                                            return true;
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
            advance(lexer);
        }

        lexer->result_symbol = TOOL_CONTENT;
        return true;
    }
