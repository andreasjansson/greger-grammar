  //  This callback handles language injection

  //  and should return a `HighlightConfiguration`.

  let  res  =  CONFIGS . get ( lang ) ;
  if  ! res. is_some ( )  {
  warn! ( " Couldn't find treesitter grammar for `{lang}` to inject " ) ;
  }
             res

