                          (content . ,(alist-get 'text (car content))))
                        dialog)
                (push `((role . "assistant")
                        (content . ,content))
                      dialog)))
            (setq pending-assistant-content '()))
          (let ((tool-result-data (greger-tree-sitter--extract-tool-result section)))
            (push `((role . "user")
                    (content . (,tool-result-data)))
                  dialog)))

