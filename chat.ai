                if (lexer->lookahead == 't') {
                    advance(lexer);
                    if (lexer->lookahead == 'o') {
                        advance(lexer);
                        if (lexer->lookahead == 'o') {
                            advance(lexer);
                            if (lexer->lookahead == 'l') {
