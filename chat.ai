    )),

    citation_entry: $ => seq(
      '###', /[^\r\n]*/,
      repeat(/[^\r\n#]+/),
    ),

    safe_shell_commands: $ => seq(
      '<safe-shell-commands>',
      repeat(/[^\r\n<]+/),
      '</safe-shell-commands>',
    ),
  }
});
