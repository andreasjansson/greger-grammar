                                    advance(lexer);
                                    if (lexer->lookahead == '.') {
                                        advance(lexer);

                                        // Check if the ID matches
                                        char close_id[256];
                                        int close_id_len = 0;
                                        while (lexer->lookahead != '>' && lexer->lookahead != 0 && close_id_len < 255) {
                                            close_id[close_id_len++] = lexer->lookahead;
                                            advance(lexer);
